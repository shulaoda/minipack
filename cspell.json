{
  "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
  "ignorePaths": [
    "**/node_modules",
    "**/dist",
    "*.snap",
    "*.node",
    "*.tsbuildinfo",
    "*.json",
    "*.toml",
    "*.css",
    "*.svg",
    "*.data",
    "*.d.ts",
    "*.wasm",
    "*.text",
    "*.go",
    "/target",
    "/temp",
    "/tmp",
    "/pnpm-lock.yaml"
  ],
  "languageSettings": [
    {
      // VSCode languageId. i.e. typescript, java, go, cpp, javascript, markdown, latex
      // * will match against any file type.
      "languageId": "*",
      // Language locale. i.e. en-US, de-AT, or ru. * will match all locales.
      // Multiple locales can be specified like: "en, en-US" to match both English and English US.
      "locale": "*",
      // To exclude patterns, add them to "ignoreRegExpList"
      "ignoreRegExpList": [
        "/import\\s.*/"
      ]
    }
  ],
  "words": [
    "arcstr",
    "binstall",
    "bitflags",
    "clippy",
    "codegen",
    "Colour",
    "conflictless",
    "Corasick",
    "daachorse",
    "dashmap",
    "deconflict",
    "esbuild",
    "extname",
    "Finalizer",
    "hashers",
    "importee",
    "indexmap",
    "itertools",
    "itoa",
    "memchr",
    "memmem",
    "metas",
    "minipack",
    "multizip",
    "onwarn",
    "peekable",
    "realpath",
    "Renamer",
    "renderable",
    "repr",
    "rolldown",
    "rollup",
    "rsplit",
    "Rstr",
    "rustc",
    "shulaoda",
    "smallvec",
    "threejs",
    "unspanned"
  ],
  "ignoreWords": [
    "Jeqs",
    "Gwetwem",
    "bpwli",
    "mxrh"
  ]
}
